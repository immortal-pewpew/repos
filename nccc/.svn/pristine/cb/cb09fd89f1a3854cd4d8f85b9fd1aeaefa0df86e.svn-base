#loading {
   	display: inline-block;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 250px;
    height: 100px;
    margin: auto;
    background-color: blue;
    text-align: center;
    z-index: 99;
}

#loading-image {
	z-index: 100;
}



.dialogbox{
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}






