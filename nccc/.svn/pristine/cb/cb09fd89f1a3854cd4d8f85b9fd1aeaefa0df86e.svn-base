	font-family: Courier; 
	display: block; 
	font-size: 12px !important;
}

.straightline{
	display: inline-block;
	margin-bottom: 20px;
}



.notify{
  text-align:center;
  font-size:16px;
  color:darkred;
  /*letter-spacing: -7px;*/
  font-weight:700;
  /*text-transform:uppercase;*/
  animation:blur 1.75s ease-out infinite;
  text-shadow:0px 0px 5px yellow,
      0px 0px 7px #006400;
}

@keyframes blur{
  from{
      text-shadow:0px 0px 10px #006400,
      0px 0px 10px #006400, 
      0px 0px 25px #006400,
      0px 0px 25px #006400,
      0px 0px 25px #006400,
      0px 0px 25px #006400,
      0px 0px 25px #006400,
      0px 0px 25px #006400,
      0px 0px 50px #006400,
      0px 0px 50px #006400,
      0px 0px 50px #7B96B8,
      0px 0px 150px #7B96B8,
      0px 10px 100px #7B96B8,
      0px 10px 100px #7B96B8,
      0px 10px 100px #7B96B8,
      0px 10px 100px #7B96B8,
      0px -10px 100px #7B96B8,
      0px -10px 100px #7B96B8;}
}

