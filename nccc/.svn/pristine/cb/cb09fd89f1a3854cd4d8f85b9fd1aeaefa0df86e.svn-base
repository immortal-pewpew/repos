/*Modal Content*/	
.modalcontent {
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size: 100%;
    opacity: 0.6;
    filter:alpha(opacity=80);
	/*padding: 20px;*/
	
	background-color: #F7F7F7;
	/*background-color: #FFF;*/
	
	margin: 0px auto 10px;
	border-radius: 2px;
	box-shadow: rgba(0, 0, 0, 0.298039) 0px 2px 2px;
	overflow: hidden;
	
    color: #000000;
}


