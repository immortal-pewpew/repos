.msgdiv {
    display:inline-block;
    max-width: 75%;
    width: auto;
    padding: 0.2em 0.6em 0.3em;
    border-radius: 0.25em;
    
}

.pwdresetfield{
	font-family: Courier;
	font-weight: bold; 
	display: block; 
	font-size: 12px !important; 
	
	/*TEXTFIELD DESIGN*/
	
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border: 1px solid lightblue;
    background: #E0FFFF;
    box-shadow: inset 0px 0px 15px cyan;
    -moz-box-shadow: inset 0px 0px 15px cyan;
    -webkit-box-shadow: inset 0px 0px 15px cyan;
	
}

.msgdiv-fill
{
    float:left;
    height:inherit;
    width: 25%;
    
}

