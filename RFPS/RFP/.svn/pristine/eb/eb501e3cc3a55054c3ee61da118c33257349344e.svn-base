/*These are just styles used for the example for the content.*/	
.left {
	float:left;
	width:500px;
	background:#ccff66;
	}
	
.right {
	float:left;
	width:200px;
	clear:right;
	background:#66ff33;
	}
	
.left p, .right p {
	padding:10px;
	}
	
	
.btn-file {
    position: relative;
    overflow: hidden;
}

.btn-file input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    font-size: 100px;
    text-align: right;
    opacity: 0;
    outline: none;
    background: white;
    cursor: inherit;
    display: block;
}	


.image {
    width: 200px;
    
    height: 150px;
    background: #444;
    
    /*margin: 0 auto;*/
    
    margin-left: 0px !important;
    margin-right: 0px !important;
    
    margin-top: 5px;
    margin-bottom: 10px;
    
    
    display: block;
    float: center;
}

